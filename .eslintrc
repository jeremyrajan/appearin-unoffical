{
  "extends": "airbnb/base",
  "rules" : {
    "comma-dangle" : [1, "never"],
    "no-unused-vars" : 1,
    "object-shorthand" : 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-restricted-syntax": [0, "ForInStatement"]
  },
  "globals": {
    "$": true
  }
}
